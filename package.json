{
  "name": "@hatsy/http-header-value",
  "version": "3.5.3",
  "description": "HTTP header value parser",
  "keywords": [
    "http",
    "header"
  ],
  "homepage": "https://github.com/hatsyjs/http-header-value",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/http-header-value.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/fun-events/issues"
  },
  "type": "module",
  "main": "./dist/http-header-value.cjs",
  "module": "./dist/http-header-value.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/http-header-value.js",
      "require": "./dist/http-header-value.cjs"
    },
    "./headers": {
      "import": "./dist/http-header-value.headers.js",
      "require": "./dist/http-header-value.headers.cjs"
    },
    "./node": {
      "import": "./dist/http-header-value.node.js",
      "require": "./dist/http-header-value.node.cjs"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@run-z/eslint-config": "^1.2.1",
    "@run-z/rollup-helpers": "^1.1.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-plugin-jest": "^24.3.6",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.47.0",
    "rollup-plugin-flat-dts": "^1.2.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "run-z": "^1.7.2",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.5",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf 'index.d.ts?(.map)' '*/index.d.ts?(.map)' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}
