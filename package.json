{
  "name": "@hatsy/http-header-value",
  "version": "3.6.0",
  "description": "HTTP header value parser",
  "keywords": [
    "http",
    "header"
  ],
  "homepage": "https://github.com/hatsyjs/http-header-value",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/http-header-value.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/fun-events/issues"
  },
  "type": "module",
  "main": "./dist/http-header-value.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./dist/http-header-value.js",
    "./headers": "./dist/http-header-value.headers.js",
    "./node": "./dist/http-header-value.node.js"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@jest/globals": "^27.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@run-z/eslint-config": "^1.3.0",
    "@run-z/rollup-helpers": "^1.1.1",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "gh-pages": "^3.2.3",
    "jest": "^27.1.0",
    "jest-junit": "^12.2.0",
    "jest-mock": "^27.1.0",
    "remark-cli": "^10.0.0",
    "remark-lint-list-item-indent": "^3.0.1",
    "remark-lint-no-shortcut-reference-link": "^3.0.1",
    "remark-preset-lint-recommended": "^6.0.1",
    "rollup": "^2.56.3",
    "rollup-plugin-flat-dts": "^1.3.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "run-z": "^1.9.2",
    "shx": "^0.3.3",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typedoc": "^0.21.9",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup -c",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf 'index.d.ts?(.map)' '*/index.d.ts?(.map)' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z + lint:md --and eslint .",
    "lint:md": "run-z +z --then remark .",
    "test": "run-z +z env:NODE_OPTIONS='--experimental-vm-modules --no-warnings' --then jest",
    "z": "run-z +cmd:rollup,+cmd:typedoc,+cmd:eslint,+cmd:jest"
  }
}
