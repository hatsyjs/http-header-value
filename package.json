{
  "name": "@hatsy/http-header-value",
  "version": "3.4.0",
  "description": "HTTP header value parser",
  "keywords": [
    "http",
    "header"
  ],
  "homepage": "https://github.com/hatsyjs/http-header-value",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/http-header-value.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/fun-events/issues"
  },
  "main": "dist/http-header-value.js",
  "module": "dist/http-header-value.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/http-header-value.mjs",
      "require": "./dist/http-header-value.js"
    },
    "./headers": {
      "import": "./dist/http-header-value.headers.mjs",
      "require": "./dist/http-header-value.headers.js"
    },
    "./node": {
      "import": "./dist/http-header-value.node.mjs",
      "require": "./dist/http-header-value.node.js"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@proc7ts/eslint-config": "^2.0.4",
    "@proc7ts/rollup-helpers": "^1.0.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.13",
    "@types/node": "^12.12.54",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "codecov": "^3.7.2",
    "eslint": "^7.8.1",
    "eslint-plugin-jest": "^23.20.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "rollup": "^2.26.10",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "run-z": "^1.1.0",
    "shx": "^0.3.2",
    "ts-jest": "^26.3.0",
    "tslib": "^2.0.1",
    "typedoc": "^0.19.0",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf d.ts dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}
